	</servlet>
	<servlet>
		<servlet-name>SelectJobByCidServlet</servlet-name>
		<display-name>SelectJobByCidServlet</display-name>
		<description></description>
		<servlet-class>me.lucheng.xiaowei.servlet.SelectJobByCidServlet</servlet-class>
	</servlet>
	<servlet>
